# Snyk (https://snyk.io) policy file, patches or ignores known vulnerabilities.
version: v1.12.0
# ignores vulnerabilities until expiry date; change duration by modifying expiry date
ignore:
  'npm:ms:20170412':
    - '*':
        reason: Not exploitable using elephant-harness
        expires: 2017-12-31T00:00:00.000Z
# patches apply the minimum changes required to fix a vulnerability
patch:
  'npm:debug:20170905':
    - nwjs > extract-zip > debug:
        patched: '2017-09-30T23:19:09.116Z'
  'npm:lodash:20180130':
    - nwjs > nwjs-versions > aimer > cheerio > lodash:
        patched: '2018-07-07T08:08:39.874Z'
